<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
            prefWidth="350.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
    <top>
        <Label text="Taxa Selection" BorderPane.alignment="CENTER"/>
    </top>
    <center>
        <HBox BorderPane.alignment="CENTER">
            <children>
                <VBox alignment="CENTER">
                    <children>
                        <Label prefHeight="20.0" text="Active"/>
                        <ListView prefWidth="500.0" VBox.vgrow="ALWAYS">
                            <VBox.margin>
                                <Insets left="10.0"/>
                            </VBox.margin>
                        </ListView>
                    </children>
                    <opaqueInsets>
                        <Insets/>
                    </opaqueInsets>
                </VBox>
                <VBox alignment="CENTER" prefWidth="150.0">
                    <children>
                        <Button mnemonicParsing="false" text="&gt;"/>
                        <Pane layoutX="10.0" layoutY="192.0" minWidth="50.0" prefHeight="20.0" prefWidth="50.0"/>
                        <Button mnemonicParsing="false" text="&gt;&gt;"/>
                        <Pane prefHeight="50.0" prefWidth="100.0"/>
                        <Button mnemonicParsing="false" text="&lt;"/>
                        <Pane layoutX="10.0" layoutY="271.0" prefHeight="20.0" prefWidth="100.0"/>
                        <Button layoutX="38.0" layoutY="229.0" mnemonicParsing="false" text="&lt;&lt;"/>
                    </children>
                </VBox>
                <VBox alignment="CENTER">
                    <children>
                        <Label prefHeight="20.0" text="Inactive"/>
                        <ListView prefWidth="500.0" VBox.vgrow="ALWAYS">
                            <VBox.margin>
                                <Insets right="10.0"/>
                            </VBox.margin>
                        </ListView>
                    </children>
                    <opaqueInsets>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                    </opaqueInsets>
                </VBox>
            </children>
        </HBox>
    </center>
    <bottom>
        <ButtonBar prefHeight="40.0" BorderPane.alignment="CENTER">
            <buttons>
                <Button mnemonicParsing="false" text="Cancel"/>
                <Button mnemonicParsing="false" text="Apply"/>
            </buttons>
            <padding>
                <Insets right="10.0"/>
            </padding>
        </ButtonBar>
    </bottom>
</BorderPane>
