<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<GridPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="180.0"
          prefWidth="300.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.111"
          fx:controller="splitstree5.gui.dagview.NewNodeDialogController">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="296.0" minWidth="10.0" prefWidth="124.0"/>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="503.0" minWidth="10.0" prefWidth="176.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="127.0" minHeight="40.0" prefHeight="40.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="127.0" minHeight="40.0" prefHeight="40.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="251.0" minHeight="40.0" prefHeight="40.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="314.0" minHeight="40.0" prefHeight="40.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <children>
        <Label text="Target Data:" GridPane.rowIndex="1"/>
        <Label text="Algorithm:" GridPane.rowIndex="2"/>
        <ChoiceBox fx:id="targetDataChoiceBox" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
        <ChoiceBox fx:id="algorithmChoiceBox" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
        <ButtonBar prefHeight="40.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="3">
            <buttons>
                <Button fx:id="cancelButton" mnemonicParsing="false" text="Cancel"/>
                <Button fx:id="applyButton" mnemonicParsing="false" text="Apply"/>
            </buttons>
        </ButtonBar>
        <Label text="Create New Node" translateX="-50.0" GridPane.columnIndex="1"/>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
</GridPane>
