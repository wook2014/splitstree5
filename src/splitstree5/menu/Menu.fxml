<?import javafx.scene.control.*?>
<?import javafx.scene.input.*?>
<MenuBar fx:id="menuBar" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1"
         fx:controller="splitstree5.menu.MenuController">
    <menus>
        <Menu fx:id="fileMenu" text="File">
            <items>
                <MenuItem fx:id="newMenuItem" text="New...">
                    <accelerator>
                        <KeyCodeCombination alt="UP" code="N" control="UP" meta="UP" shift="UP" shortcut="DOWN"/>
                    </accelerator>
                </MenuItem>
                <SeparatorMenuItem mnemonicParsing="false"/>
                <MenuItem fx:id="openMenuItem" mnemonicParsing="false" text="Open...">
                    <accelerator>
                        <KeyCodeCombination alt="UP" code="O" control="UP" meta="UP" shift="UP" shortcut="DOWN"/>
                    </accelerator>
                </MenuItem>
                <Menu fx:id="openRecentMenu" text="Open Recent"/>
                <SeparatorMenuItem mnemonicParsing="false"/>
                <MenuItem fx:id="importMenuItem" text="Import...">
                    <accelerator>
                        <KeyCodeCombination alt="UP" code="I" control="UP" meta="UP" shift="UP" shortcut="DOWN"/>
                    </accelerator>
                </MenuItem>
                <MenuItem fx:id="importGenomesMenuItem" text="Import Genomes..."/>
                <MenuItem fx:id="replaceDataMenuItem" text="Replace Data...">
                    <accelerator>
                        <KeyCodeCombination alt="UP" code="I" control="UP" meta="UP" shift="DOWN" shortcut="DOWN"/>
                    </accelerator>
                </MenuItem>
                <SeparatorMenuItem mnemonicParsing="false"/>
                <MenuItem fx:id="inputEditorMenuItem" text="Input Editor...">
                    <accelerator>
                        <KeyCodeCombination alt="UP" code="U" control="UP" meta="UP" shift="UP" shortcut="DOWN"/>
                    </accelerator>
                </MenuItem>
                <SeparatorMenuItem mnemonicParsing="false"/>
                <MenuItem fx:id="saveMenuItem" text="Save">
                    <accelerator>
                        <KeyCodeCombination alt="UP" code="S" control="UP" meta="UP" shift="UP" shortcut="DOWN"/>
                    </accelerator>
                </MenuItem>
                <MenuItem fx:id="saveAsMenuItem" text="Save As...">
                    <accelerator>
                        <KeyCodeCombination alt="UP" code="S" control="UP" meta="UP" shift="DOWN" shortcut="DOWN"/>
                    </accelerator>
                </MenuItem>
                <SeparatorMenuItem mnemonicParsing="false"/>
                <MenuItem fx:id="exportMenuItem" text="Export...">
                    <accelerator>
                        <KeyCodeCombination alt="UP" code="E" control="UP" meta="UP" shift="UP" shortcut="DOWN"/>
                    </accelerator>
                </MenuItem>
                <MenuItem fx:id="exportImageMenuItem" text="Export Image...">
                    <accelerator>
                        <KeyCodeCombination alt="UP" code="E" control="UP" meta="UP" shift="DOWN" shortcut="DOWN"/>
                    </accelerator>
                </MenuItem>
                <MenuItem fx:id="exportWorkflowMenuItem" text="Export Workflow..."/>
                <SeparatorMenuItem mnemonicParsing="false"/>
                <MenuItem fx:id="pageSetupMenuItem" text="Page Setup..."/>
                <MenuItem fx:id="printMenuitem" text="Print...">
                    <accelerator>
                        <KeyCodeCombination alt="UP" code="P" control="UP" meta="UP" shift="DOWN" shortcut="DOWN"/>
                    </accelerator>
                </MenuItem>
                <SeparatorMenuItem mnemonicParsing="false"/>
                <Menu fx:id="toolsMenu" text="Tools">
                    <items>
                        <MenuItem fx:id="importMultipleTreeFilesMenuItem" text="Import Trees from Multiple Files..."/>
                        <MenuItem fx:id="GroupIdenticalHaplotypesFilesMenuItem" text="Group Identical Haplotypes...">
                        </MenuItem>
                    </items>
                </Menu>
                <SeparatorMenuItem mnemonicParsing="false"/>
                <MenuItem fx:id="closeMenuItem" text="Close">
                    <accelerator>
                        <KeyCodeCombination alt="UP" code="W" control="UP" meta="UP" shift="UP" shortcut="DOWN"/>
                    </accelerator>
                </MenuItem>
                <MenuItem fx:id="quitMenuItem" text="Quit">
                    <accelerator>
                        <KeyCodeCombination alt="UP" code="Q" control="UP" meta="UP" shift="UP" shortcut="DOWN"/>
                    </accelerator>
                </MenuItem>
            </items>
        </Menu>
        <Menu fx:id="editMenu" text="Edit">
            <items>
                <MenuItem fx:id="undoMenuItem" text="Undo">
                    <accelerator>
                        <KeyCodeCombination alt="UP" code="Z" control="UP" meta="UP" shift="UP" shortcut="DOWN"/>
                    </accelerator>
                </MenuItem>
                <MenuItem fx:id="redoMenuItem" text="Redo">
                    <accelerator>
                        <KeyCodeCombination alt="UP" code="Z" control="UP" meta="UP" shift="DOWN" shortcut="DOWN"/>
                    </accelerator>
                </MenuItem>
                <SeparatorMenuItem mnemonicParsing="false"/>
                <MenuItem fx:id="cutMenuItem" text="Cut">
                    <accelerator>
                        <KeyCodeCombination alt="UP" code="X" control="UP" meta="UP" shift="UP" shortcut="DOWN"/>
                    </accelerator>
                </MenuItem>
                <MenuItem fx:id="copyMenuItem" text="Copy">
                    <accelerator>
                        <KeyCodeCombination alt="UP" code="C" control="UP" meta="UP" shift="UP" shortcut="DOWN"/>
                    </accelerator>
                </MenuItem>
                <MenuItem fx:id="copyImageMenuItem" text="Copy Image">
                    <accelerator>
                        <KeyCodeCombination alt="UP" code="C" control="UP" meta="UP" shift="DOWN" shortcut="DOWN"/>
                    </accelerator>
                </MenuItem>
                <MenuItem fx:id="pasteMenuItem" text="Paste">
                    <accelerator>
                        <KeyCodeCombination alt="UP" code="V" control="UP" meta="UP" shift="UP" shortcut="DOWN"/>
                    </accelerator>
                </MenuItem>
                <SeparatorMenuItem mnemonicParsing="false"/>
                <MenuItem fx:id="duplicateMenuItem" text="Duplicate">
                    <accelerator>
                        <KeyCodeCombination alt="UP" code="D" control="UP" meta="UP" shift="UP" shortcut="DOWN"/>
                    </accelerator>
                </MenuItem>
                <SeparatorMenuItem mnemonicParsing="false"/>
                <MenuItem fx:id="deleteMenuItem" text="Delete">
                    <accelerator>
                        <KeyCodeCombination alt="UP" code="BACK_SPACE" control="UP" meta="UP" shift="UP"
                                            shortcut="DOWN"/>
                    </accelerator>
                </MenuItem>
                <SeparatorMenuItem mnemonicParsing="false"/>
                <MenuItem fx:id="findMenuItem" text="Find...">
                    <accelerator>
                        <KeyCodeCombination alt="UP" code="F" control="UP" meta="UP" shift="UP" shortcut="DOWN"/>
                    </accelerator>
                </MenuItem>
                <MenuItem fx:id="findAgainMenuItem" text="Find Again">
                    <accelerator>
                        <KeyCodeCombination alt="UP" code="G" control="UP" meta="UP" shift="UP" shortcut="DOWN"/>
                    </accelerator>
                </MenuItem>
                <MenuItem fx:id="replaceMenuItem" text="Replace...">
                </MenuItem>
                <SeparatorMenuItem mnemonicParsing="false"/>
                <MenuItem fx:id="gotoLineMenuItem" text="Goto Line...">
                    <accelerator>
                        <KeyCodeCombination alt="UP" code="G" control="UP" meta="UP" shift="DOWN" shortcut="DOWN"/>
                    </accelerator>
                </MenuItem>
                <SeparatorMenuItem mnemonicParsing="false"/>
                <MenuItem fx:id="preferencesMenuItem" text="Preferences...">
                    <accelerator>
                        <KeyCodeCombination alt="UP" code="P" control="UP" meta="UP" shift="DOWN" shortcut="DOWN"/>
                    </accelerator>
                </MenuItem>
            </items>
        </Menu>
        <Menu text="Select">
            <items>
                <MenuItem fx:id="selectAllMenuItem" text="Select All">
                    <accelerator>
                        <KeyCodeCombination alt="UP" code="A" control="UP" meta="UP" shift="UP" shortcut="DOWN"/>
                    </accelerator>
                </MenuItem>
                <MenuItem fx:id="selectNoneMenuItem" text="Select None">
                    <accelerator>
                        <KeyCodeCombination alt="UP" code="A" control="UP" meta="UP" shift="DOWN" shortcut="DOWN"/>
                    </accelerator>
                </MenuItem>
                <SeparatorMenuItem mnemonicParsing="false"/>
                <MenuItem fx:id="selectAllNodesMenuItem" text="All Nodes"/>
                <MenuItem fx:id="selectAllLabeledNodesMenuItem" text="All Labeled Nodes">
                    <accelerator>
                        <KeyCodeCombination alt="UP" code="L" control="UP" meta="UP" shift="DOWN" shortcut="DOWN"/>
                    </accelerator>
                </MenuItem>
                <MenuItem fx:id="selectAllBelowMenuItem" text="All Nodes Below">
                    <accelerator>
                        <KeyCodeCombination alt="UP" code="B" control="UP" meta="UP" shift="UP" shortcut="DOWN"/>
                    </accelerator>
                </MenuItem>
                <SeparatorMenuItem mnemonicParsing="false"/>
                <MenuItem fx:id="selectBracketsMenuItem" text="Brackets">
                    <accelerator>
                        <KeyCodeCombination alt="UP" code="B" control="UP" meta="UP" shift="UP" shortcut="DOWN"/>
                    </accelerator>
                </MenuItem>
                <SeparatorMenuItem mnemonicParsing="false"/>
                <MenuItem fx:id="invertNodeSelectionMenuItem" mnemonicParsing="false" text="Invert Node Selection"/>
                <MenuItem fx:id="deselectAllNodesMenuItem" mnemonicParsing="false" text="Deselect Nodes"/>
                <SeparatorMenuItem mnemonicParsing="false"/>
                <MenuItem fx:id="selectAllEdgesMenuItem" text="All Edges"/>
                <MenuItem fx:id="selectAllLabeledEdgesMenuItem" mnemonicParsing="false" text="All Labeled Edges"/>
                <MenuItem fx:id="selectAllEdgesBelowMenuItem" mnemonicParsing="false" text="All Edges Below"/>
                <SeparatorMenuItem mnemonicParsing="false"/>
                <MenuItem fx:id="invertEdgeSelectionMenuItem" mnemonicParsing="false" text="Invert Edge Selection"/>
                <MenuItem fx:id="deselectEdgesMenuItem" mnemonicParsing="false" text="Deselect Edges"/>
                <SeparatorMenuItem mnemonicParsing="false"/>
                <MenuItem fx:id="selectFromPreviousMenuItem" text="From Previous">
                    <accelerator>
                        <KeyCodeCombination alt="UP" code="P" control="UP" meta="UP" shift="UP" shortcut="DOWN"/>
                    </accelerator>
                </MenuItem>
            </items>
        </Menu>
        <Menu text="View">
            <items>
                <MenuItem fx:id="increaseFontSizeMenuItem" text="Increase Font Size">
                    <accelerator>
                        <KeyCodeCombination alt="UP" code="PLUS" control="UP" meta="UP" shift="UP" shortcut="DOWN"/>
                    </accelerator>
                </MenuItem>
                <MenuItem fx:id="decreaseFontSizeMenuItem" text="Decrease Font Size">
                    <accelerator>
                        <KeyCodeCombination alt="UP" code="MINUS" control="UP" meta="UP" shift="UP" shortcut="DOWN"/>
                    </accelerator>
                </MenuItem>
                <SeparatorMenuItem mnemonicParsing="false"/>
                <MenuItem fx:id="zoomInMenuItem" text="Zoom In">
                    <accelerator>
                        <KeyCodeCombination alt="UP" code="UP" control="UP" meta="UP" shift="UP" shortcut="DOWN"/>
                    </accelerator>
                </MenuItem>
                <MenuItem fx:id="zoomOutMenuItem" text="Zoom Out">
                    <accelerator>
                        <KeyCodeCombination alt="UP" code="DOWN" control="UP" meta="UP" shift="UP" shortcut="DOWN"/>
                    </accelerator>
                </MenuItem>
                <SeparatorMenuItem mnemonicParsing="false"/>
                <MenuItem fx:id="resetMenuItem" mnemonicParsing="false" text="Reset">
                    <accelerator>
                        <KeyCodeCombination alt="UP" code="DIGIT0" control="UP" meta="UP" shift="UP" shortcut="DOWN"/>
                    </accelerator>
                </MenuItem>
                <SeparatorMenuItem mnemonicParsing="false"/>
                <MenuItem fx:id="rotateLeftMenuItem" text="Rotate Left">
                    <accelerator>
                        <KeyCodeCombination alt="UP" code="LEFT" control="UP" meta="UP" shift="UP" shortcut="DOWN"/>
                    </accelerator>
                </MenuItem>
                <MenuItem fx:id="rotateRightMenuItem" text="Rotate Right">
                    <accelerator>
                        <KeyCodeCombination alt="UP" code="RIGHT" control="UP" meta="UP" shift="UP" shortcut="DOWN"/>
                    </accelerator>
                </MenuItem>
                <MenuItem fx:id="flipMenuItem" text="Flip">
                </MenuItem>
                <SeparatorMenuItem mnemonicParsing="false"/>
                <CheckMenuItem fx:id="wrapTextMenuItem" text="Wrap Text">
                </CheckMenuItem>
                <SeparatorMenuItem mnemonicParsing="false"/>
                <MenuItem fx:id="formatNodesMenuItem" text="Format...">
                    <accelerator>
                        <KeyCodeCombination alt="UP" code="J" control="UP" meta="UP" shift="DOWN" shortcut="DOWN"/>
                    </accelerator>
                </MenuItem>
                <SeparatorMenuItem mnemonicParsing="false"/>
                <MenuItem fx:id="layoutLabelsMenuItem" text="Layout Labels">
                    <accelerator>
                        <KeyCodeCombination alt="UP" code="L" control="UP" meta="UP" shift="UP" shortcut="DOWN"/>
                    </accelerator>
                </MenuItem>
                <CheckMenuItem fx:id="sparseLabelsCheckMenuItem" text="Sparse Labels"/>
                <SeparatorMenuItem mnemonicParsing="false"/>
                <CheckMenuItem fx:id="showScaleBarMenuItem" text="Show Scale Bar"/>
                <SeparatorMenuItem mnemonicParsing="false"/>
                <MenuItem fx:id="fullScreenMenuItem" text="Enter FullScreen">
                    <accelerator>
                        <KeyCodeCombination alt="UP" code="F" control="UP" meta="UP" shift="DOWN" shortcut="DOWN"/>
                    </accelerator>
                </MenuItem>
            </items>
        </Menu>
        <Menu text="Data">
            <items>
                <MenuItem fx:id="filterTaxaMenuItem" text="Filter Taxa"/>
                <MenuItem fx:id="filterCharactersMenuItem" text="Filter Characters"/>
                <MenuItem fx:id="filterTreesMenuItem" text="Filter Trees"/>
                <MenuItem fx:id="filterSplitsMenuItem" text="Filter Splits"/>
                <SeparatorMenuItem mnemonicParsing="false"/>
                <MenuItem fx:id="traitsMenuItem" text="Traits"/>
            </items>
        </Menu>
        <Menu text="Distances">
            <items>
                <MenuItem fx:id="uncorrectedPMenuItem" text="Uncorrected P"/>
                <MenuItem fx:id="logDetMenuItem" text="Log Det"/>
                <SeparatorMenuItem mnemonicParsing="false"/>
                <MenuItem fx:id="hky85MenuItem" text="HKY 85"/>
                <MenuItem fx:id="jukesCantorMenuItem" text="Jukes Cantor"/>
                <MenuItem fx:id="k2pMenuItem" text="K2P"/>
                <MenuItem fx:id="k3stMenuItem" text="K3ST"/>
                <MenuItem fx:id="f81MenuItem" text="F81"/>
                <MenuItem fx:id="f84MenuItem" text="F84"/>
                <SeparatorMenuItem mnemonicParsing="false"/>
                <MenuItem fx:id="proteinMLDistanceMenuItem" text="Protein ML Dist"/>
                <SeparatorMenuItem mnemonicParsing="false"/>
                <MenuItem fx:id="geneContentDistanceMenuItem" text="Gene Content Distance"/>
            </items>
        </Menu>
        <Menu text="Tree">
            <items>
                <MenuItem fx:id="njMenuItem" text="NJ"/>
                <MenuItem fx:id="bioNJMenuItem" text="BioNJ"/>
                <MenuItem fx:id="upgmaMenuItem" text="UPGMA"/>
                <SeparatorMenuItem mnemonicParsing="false"/>
                <MenuItem fx:id="bunemanTreeMenuItem" text="Buneman Tree"/>
                <SeparatorMenuItem mnemonicParsing="false"/>
                <MenuItem fx:id="selectTreeMenuItem" text="Select Tree"/>
                <MenuItem fx:id="consensusTreeMenuItem" text="Consensus Tree"/>
                <SeparatorMenuItem mnemonicParsing="false"/>
                <MenuItem fx:id="rootByOutgroupMenuItem" text="Root By Outgroup"/>
                <MenuItem fx:id="rootByMidpointMenuItem" text="Root By Midpoint"/>
                <SeparatorMenuItem mnemonicParsing="false"/>
                <MenuItem fx:id="treeViewMenuItem" text="Tree View"/>
                <MenuItem fx:id="treeGridMenuItem" text="Tree Grid"/>
                <SeparatorMenuItem mnemonicParsing="false"/>
                <MenuItem fx:id="tanglegramMenuItem" text="Tanglegram"/>
            </items>
        </Menu>
        <Menu text="Network">
            <items>
                <MenuItem fx:id="neighborNetMenuItem" text="Neighbor Net"/>
                <MenuItem fx:id="splitDecompositionMenuItem" text="Split Decomposition"/>
                <MenuItem fx:id="parsimonySplitsMenuItem" text="Parsimony Splits"/>
                <SeparatorMenuItem mnemonicParsing="false"/>
                <MenuItem fx:id="consensusNetworkMenuItem" text="Consensus Network"/>
                <MenuItem fx:id="filteredSuperNetworkMenuItem" text="Filtered Super Network"/>
                <SeparatorMenuItem mnemonicParsing="false"/>
                <MenuItem fx:id="medianNetworkMenuItem" text="Median Network"/>
                <MenuItem fx:id="medianJoiningMenuItem" text="Median Joining Network"/>
                <MenuItem fx:id="minSpanningNetworkMenuItem" text="Min Spanning Network"/>
                <SeparatorMenuItem mnemonicParsing="false"/>
                <MenuItem fx:id="consensusClusterNetworkMenuItem" text="Consensus Cluster Network"/>
                <MenuItem fx:id="hybridizationNetworkMenuItem" text="Hybridization Network"/>
                <SeparatorMenuItem mnemonicParsing="false"/>
                <MenuItem fx:id="splitsNetworkViewMenuItem" text="Splits Network View"/>
                <MenuItem fx:id="haplotypeNetworkViewMenuItem" text="Haplotype Network View"/>
                <SeparatorMenuItem mnemonicParsing="false"/>
                <MenuItem fx:id="show3DViewerMenuItem" text="3D Viewer"/>
                <MenuItem fx:id="relaxMenuItem" text="Relax">
                    <accelerator>
                        <KeyCodeCombination alt="UP" code="R" control="UP" meta="UP" shift="UP" shortcut="DOWN"/>
                    </accelerator>
                </MenuItem>
            </items>
        </Menu>
        <Menu text="PCoA">
            <items>
                <MenuItem fx:id="pcoaMenuItem" text="PCoA"/>
                <SeparatorMenuItem mnemonicParsing="false"/>
                <MenuItem fx:id="brayCurtisMenuItem" text="Bray Curtis"/>
                <MenuItem fx:id="jsdMenuItem" text="JSD"/>
                <MenuItem fx:id="bootstrappingMenuItem" text="Euclidean"/>
            </items>
        </Menu>
        <Menu text="Analysis">
            <items>
                <MenuItem fx:id="showBootStrapTreeMenuItem" text="Show Bootstrap Tree..."/>
                <MenuItem fx:id="showBootStrapNetworkMenuItem" text="Show Bootstrap Network..."/>
                <SeparatorMenuItem mnemonicParsing="false"/>
                <MenuItem fx:id="estimateInvariableSitesMenuItem" text="Esimate Invariable Sites..."/>
                <MenuItem fx:id="computePhylogeneticDiversityMenuItem" text="Compute Phylogenetic Diversity"/>
                <MenuItem fx:id="computeDeltaScoreMenuItem" text="Compute Delta Score"/>
                <SeparatorMenuItem mnemonicParsing="false"/>
                <MenuItem fx:id="showWorkflowMenuItem" text="Show Workflow"/>
            </items>
        </Menu>
        <Menu fx:id="windowMenu" text="Window">
            <items>
                <MenuItem fx:id="showMessageWindowMenuItem" text="Show Message Window..."/>
            </items>
        </Menu>
        <Menu text="Help">
            <items>
                <MenuItem fx:id="checkForUpdatesMenuItem" text="Check For Updates..."/>
                <SeparatorMenuItem mnemonicParsing="false"/>
                <MenuItem fx:id="aboutMenuItem" text="About...">
                    <accelerator>
                        <KeyCodeCombination alt="UP" code="H" control="UP" meta="UP" shift="UP" shortcut="DOWN"/>
                    </accelerator>
                </MenuItem>
                <MenuItem fx:id="communityWebsiteMenuItem" text="Community Webpage..."/>
            </items>
        </Menu>
    </menus>
</MenuBar>
