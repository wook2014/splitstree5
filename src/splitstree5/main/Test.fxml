<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            prefHeight="400.0" prefWidth="600.0">

    <Button fx:id="collapseAllButton" maxHeight="18.0" maxWidth="18.0"
            minHeight="18.0" minWidth="18.0" mnemonicParsing="false"
            prefHeight="18.0" prefWidth="18.0">
        <graphic>
            <ImageView fitHeight="16.0" fitWidth="16.0" pickOnBounds="true"
                       preserveRatio="true">
                <image>
                    <Image smooth="false"
                           url="@/splitstree5/resources/icons/CollapseTreeView16.gif"/>
                </image>
            </ImageView>
        </graphic>
    </Button>


</AnchorPane>
