<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.input.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<BorderPane fx:id="borderPane" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="splitstree5.main.MainWindowController">
    <top>
        <VBox fx:id="topVBox" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
              BorderPane.alignment="CENTER">
            <children>
                <MenuBar fx:id="menuBar">
                    <menus>
                        <Menu fx:id="fileMenu" mnemonicParsing="false" text="File">
                            <items>
                                <MenuItem fx:id="newMenuItem" text="New...">
                                    <accelerator>
                                        <KeyCodeCombination alt="UP" code="N" control="UP" meta="UP" shift="UP"
                                                            shortcut="DOWN"/>
                                    </accelerator>
                                </MenuItem>
                                <SeparatorMenuItem mnemonicParsing="false"/>
                                <MenuItem fx:id="openMenuItem" mnemonicParsing="false" text="Open...">
                                    <accelerator>
                                        <KeyCodeCombination alt="UP" code="O" control="UP" meta="UP" shift="UP"
                                                            shortcut="DOWN"/>
                                    </accelerator>
                                </MenuItem>
                                <Menu fx:id="openRecentMenu" mnemonicParsing="false" text="Open Recent"/>
                                <SeparatorMenuItem mnemonicParsing="false"/>
                                <MenuItem fx:id="enterDataMenuItem" text="Enter Data">
                                    <accelerator>
                                        <KeyCodeCombination alt="UP" code="O" control="UP" meta="UP" shift="DOWN"
                                                            shortcut="DOWN"/>
                                    </accelerator>
                                </MenuItem>
                                <SeparatorMenuItem mnemonicParsing="false"/>
                                <MenuItem fx:id="saveMenuItem" text="Save">
                                    <accelerator>
                                        <KeyCodeCombination alt="UP" code="S" control="UP" meta="UP" shift="UP"
                                                            shortcut="DOWN"/>
                                    </accelerator>
                                </MenuItem>
                                <MenuItem fx:id="saveAsMenuItem" text="Save As...">
                                    <accelerator>
                                        <KeyCodeCombination alt="UP" code="S" control="UP" meta="UP" shift="DOWN"
                                                            shortcut="DOWN"/>
                                    </accelerator>
                                </MenuItem>
                                <SeparatorMenuItem mnemonicParsing="false"/>
                                <MenuItem fx:id="ImportMenuItem" mnemonicParsing="false" text="Import...">
                                    <accelerator>
                                        <KeyCodeCombination alt="UP" code="I" control="UP" meta="UP" shift="UP"
                                                            shortcut="DOWN"/>
                                    </accelerator>
                                </MenuItem>
                                <MenuItem fx:id="exportMenuItem" mnemonicParsing="false" text="Export...">
                                    <accelerator>
                                        <KeyCodeCombination alt="UP" code="E" control="UP" meta="UP" shift="UP"
                                                            shortcut="DOWN"/>
                                    </accelerator>
                                </MenuItem>
                                <MenuItem fx:id="exportImageMenuItem" mnemonicParsing="false" text="Export Image...">
                                    <accelerator>
                                        <KeyCodeCombination alt="UP" code="E" control="UP" meta="UP" shift="DOWN"
                                                            shortcut="DOWN"/>
                                    </accelerator>
                                </MenuItem>
                                <SeparatorMenuItem mnemonicParsing="false"/>
                                <MenuItem fx:id="printMenuitem" mnemonicParsing="false" text="Print">
                                    <accelerator>
                                        <KeyCodeCombination alt="UP" code="P" control="UP" meta="UP" shift="DOWN"
                                                            shortcut="DOWN"/>
                                    </accelerator>
                                </MenuItem>
                                <SeparatorMenuItem mnemonicParsing="false"/>
                                <MenuItem fx:id="closeMenuItem" text="Close">
                                    <accelerator>
                                        <KeyCodeCombination alt="UP" code="W" control="UP" meta="UP" shift="UP"
                                                            shortcut="DOWN"/>
                                    </accelerator>
                                </MenuItem>
                                <MenuItem fx:id="quitMenuItem" text="Quit">
                                    <accelerator>
                                        <KeyCodeCombination alt="UP" code="Q" control="UP" meta="UP" shift="UP"
                                                            shortcut="DOWN"/>
                                    </accelerator>
                                </MenuItem>
                            </items>
                        </Menu>
                        <Menu fx:id="editMenu" mnemonicParsing="false" text="Edit">
                            <items>
                                <MenuItem fx:id="undoMenuItem" text="Undo">
                                    <accelerator>
                                        <KeyCodeCombination alt="UP" code="Z" control="UP" meta="UP" shift="UP"
                                                            shortcut="DOWN"/>
                                    </accelerator>
                                </MenuItem>
                                <MenuItem fx:id="redoMenuItem" text="Redo">
                                    <accelerator>
                                        <KeyCodeCombination alt="UP" code="Z" control="UP" meta="UP" shift="DOWN"
                                                            shortcut="DOWN"/>
                                    </accelerator>
                                </MenuItem>
                                <SeparatorMenuItem mnemonicParsing="false"/>
                                <MenuItem fx:id="cutMenuItem" text="Cut">
                                    <accelerator>
                                        <KeyCodeCombination alt="UP" code="X" control="UP" meta="UP" shift="UP"
                                                            shortcut="DOWN"/>
                                    </accelerator>
                                </MenuItem>
                                <MenuItem fx:id="copyMenuItem" text="Copy">
                                    <accelerator>
                                        <KeyCodeCombination alt="UP" code="C" control="UP" meta="UP" shift="UP"
                                                            shortcut="DOWN"/>
                                    </accelerator>
                                </MenuItem>
                                <MenuItem fx:id="pasteMenuItem" text="Paste">
                                    <accelerator>
                                        <KeyCodeCombination alt="UP" code="V" control="UP" meta="UP" shift="UP"
                                                            shortcut="DOWN"/>
                                    </accelerator>
                                </MenuItem>
                                <SeparatorMenuItem mnemonicParsing="false"/>
                                <MenuItem fx:id="deleteMenuItem" text="Delete">
                                    <accelerator>
                                        <KeyCodeCombination alt="UP" code="BACK_SPACE" control="UP" meta="UP" shift="UP"
                                                            shortcut="DOWN"/>
                                    </accelerator>
                                </MenuItem>
                                <SeparatorMenuItem mnemonicParsing="false"/>
                                <MenuItem fx:id="selectAllMenuItem" text="Select All">
                                    <accelerator>
                                        <KeyCodeCombination alt="UP" code="A" control="UP" meta="UP" shift="UP"
                                                            shortcut="DOWN"/>
                                    </accelerator>
                                </MenuItem>
                                <MenuItem fx:id="selectNoneMenuItem" text="Select None">
                                    <accelerator>
                                        <KeyCodeCombination alt="UP" code="A" control="UP" meta="UP" shift="DOWN"
                                                            shortcut="DOWN"/>
                                    </accelerator>
                                </MenuItem>
                                <SeparatorMenuItem mnemonicParsing="false"/>
                                <MenuItem fx:id="selectAllNodesMenuItem" text="Select All Nodes"/>
                                <MenuItem fx:id="selectAllLabeledNodesMenuItem" text="Select All Labeled Nodes">
                                    <accelerator>
                                        <KeyCodeCombination alt="UP" code="L" control="UP" meta="UP" shift="UP"
                                                            shortcut="DOWN"/>
                                    </accelerator>
                                </MenuItem>
                                <MenuItem fx:id="selectAllEdgeMenuItem" text="Select All Edges"/>
                                <SeparatorMenuItem mnemonicParsing="false"/>
                                <MenuItem fx:id="selectFromPreviousMenuItem" text="Select From Previous">
                                    <accelerator>
                                        <KeyCodeCombination alt="UP" code="P" control="UP" meta="UP" shift="UP"
                                                            shortcut="DOWN"/>
                                    </accelerator>
                                </MenuItem>
                                <SeparatorMenuItem mnemonicParsing="false"/>
                                <MenuItem fx:id="findMenuItem" text="Find / Replace...">
                                    <accelerator>
                                        <KeyCodeCombination alt="UP" code="F" control="UP" meta="UP" shift="UP"
                                                            shortcut="DOWN"/>
                                    </accelerator>
                                </MenuItem>
                                <MenuItem fx:id="findAgainMenuItem" text="Find Again">
                                    <accelerator>
                                        <KeyCodeCombination alt="UP" code="G" control="UP" meta="UP" shift="UP"
                                                            shortcut="DOWN"/>
                                    </accelerator>
                                </MenuItem>
                                <SeparatorMenuItem mnemonicParsing="false"/>
                                <MenuItem fx:id="gotoLineMenuItem" text="Goto Line">
                                    <accelerator>
                                        <KeyCodeCombination alt="UP" code="G" control="UP" meta="UP" shift="DOWN"
                                                            shortcut="DOWN"/>
                                    </accelerator>
                                </MenuItem>
                                <SeparatorMenuItem mnemonicParsing="false"/>
                                <MenuItem fx:id="preferencesMenuItem" text="Preferences...">
                                    <accelerator>
                                        <KeyCodeCombination alt="UP" code="P" control="UP" meta="UP" shift="DOWN"
                                                            shortcut="DOWN"/>
                                    </accelerator>
                                </MenuItem>
                            </items>
                        </Menu>
                        <Menu mnemonicParsing="false" text="View">
                            <items>
                                <MenuItem fx:id="increaseFontSizeMenuItem" text="Increase Font Size">
                                    <accelerator>
                                        <KeyCodeCombination alt="UP" code="EQUALS" control="UP" meta="UP" shift="UP"
                                                            shortcut="DOWN"/>
                                    </accelerator>
                                </MenuItem>
                                <MenuItem fx:id="decreaseFontSizeMenuItem" text="Decrease Font Size">
                                    <accelerator>
                                        <KeyCodeCombination alt="UP" code="MINUS" control="UP" meta="UP" shift="UP"
                                                            shortcut="DOWN"/>
                                    </accelerator>
                                </MenuItem>
                                <SeparatorMenuItem mnemonicParsing="false"/>
                                <MenuItem fx:id="zoomInMenuItem" text="Zoom In">
                                    <accelerator>
                                        <KeyCodeCombination alt="UP" code="UP" control="UP" meta="UP" shift="UP"
                                                            shortcut="DOWN"/>
                                    </accelerator>
                                </MenuItem>
                                <MenuItem fx:id="zoomOutMenuItem" text="Zoom Out">
                                    <accelerator>
                                        <KeyCodeCombination alt="UP" code="DOWN" control="UP" meta="UP" shift="UP"
                                                            shortcut="DOWN"/>
                                    </accelerator>
                                </MenuItem>
                                <SeparatorMenuItem mnemonicParsing="false"/>
                                <MenuItem fx:id="resetMenuItem" mnemonicParsing="false" text="Reset">
                                    <accelerator>
                                        <KeyCodeCombination alt="UP" code="DIGIT0" control="UP" meta="UP" shift="UP"
                                                            shortcut="DOWN"/>
                                    </accelerator>
                                </MenuItem>
                                <SeparatorMenuItem mnemonicParsing="false"/>
                                <MenuItem fx:id="rotateLeftMenuItem" text="Rotate Left">
                                    <accelerator>
                                        <KeyCodeCombination alt="UP" code="LEFT" control="UP" meta="UP" shift="UP"
                                                            shortcut="DOWN"/>
                                    </accelerator>
                                </MenuItem>
                                <MenuItem fx:id="rotateRightMenuItem" text="Rotate Right">
                                    <accelerator>
                                        <KeyCodeCombination alt="UP" code="RIGHT" control="UP" meta="UP" shift="UP"
                                                            shortcut="DOWN"/>
                                    </accelerator>
                                </MenuItem>
                                <MenuItem fx:id="flipMenuItem" text="Flip">
                                    <accelerator>
                                        <KeyCodeCombination alt="UP" code="F" control="UP" meta="UP" shift="DOWN"
                                                            shortcut="DOWN"/>
                                    </accelerator>
                                </MenuItem>
                                <SeparatorMenuItem mnemonicParsing="false"/>
                                <MenuItem fx:id="formatNodesMenuItem" text="Format Nodes...">
                                    <accelerator>
                                        <KeyCodeCombination alt="UP" code="F" control="UP" meta="UP" shift="DOWN"
                                                            shortcut="DOWN"/>
                                    </accelerator>
                                </MenuItem>
                                <MenuItem fx:id="formatEdgesMenuItem" text="Format Edges..."/>
                                <SeparatorMenuItem mnemonicParsing="false"/>
                                <MenuItem fx:id="layoutLabelsMenuItem" text="Layout Labels">
                                    <accelerator>
                                        <KeyCodeCombination alt="UP" code="L" control="UP" meta="UP" shift="DOWN"
                                                            shortcut="DOWN"/>
                                    </accelerator>
                                </MenuItem>
                            </items>
                        </Menu>
                        <Menu mnemonicParsing="false" text="Distances"/>
                        <Menu mnemonicParsing="false" text="Data"/>
                        <Menu mnemonicParsing="false" text="Trees"/>
                        <Menu mnemonicParsing="false" text="Networks"/>
                        <Menu mnemonicParsing="false" text="Analysis"/>
                        <Menu mnemonicParsing="false" text="Workflow">
                            <items>
                                <CheckMenuItem mnemonicParsing="false" text="Show Workflow"/>
                            </items>
                        </Menu>
                        <Menu fx:id="windowMenu" mnemonicParsing="false" text="Window">
                            <items>
                                <MenuItem fx:id="aboutMenuItem" mnemonicParsing="false" text="About...">
                                    <accelerator>
                                        <KeyCodeCombination alt="UP" code="H" control="UP" meta="UP" shift="UP"
                                                            shortcut="DOWN"/>
                                    </accelerator>
                                </MenuItem>
                            </items>
                        </Menu>
                        <Menu mnemonicParsing="false" text="Help"/>
                    </menus>
                </MenuBar>
                <ToolBar fx:id="topToolBar" minHeight="40.0" prefHeight="40.0" prefWidth="200.0">
                    <items>
                        <Button fx:id="openCloseTreeView" maxHeight="10.0" maxWidth="10.0" mnemonicParsing="false"
                                prefHeight="10.0" prefWidth="10.0"
                                style="-fx-border-width: 0; -fx-border-style: none; -fx-background-color: transparent;"
                                text="&gt;" translateX="-10.0">
                            <font>
                                <Font size="10.0"/>
                            </font>
                        </Button>
                    </items>
                </ToolBar>
            </children>
        </VBox>
    </top>
    <center>
        <SplitPane fx:id="splitPane" dividerPositions="0.0" maxHeight="1.7976931348623157E308"
                   maxWidth="1.7976931348623157E308" pickOnBounds="false" BorderPane.alignment="CENTER">
            <BorderPane.margin>
                <Insets/>
            </BorderPane.margin>
            <items>
                <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
                    <children>
                        <ToolBar minHeight="0.0" minWidth="0.0" prefHeight="40.0" prefWidth="200.0">
                            <items>
                                <Button fx:id="expandAllButton" maxHeight="18.0" maxWidth="18.0" minHeight="18.0"
                                        minWidth="18.0" mnemonicParsing="false" prefHeight="18.0" prefWidth="18.0">
                                    <graphic>
                                        <ImageView fitHeight="16.0" fitWidth="16.0" pickOnBounds="true"
                                                   preserveRatio="true">
                                            <image>
                                                <Image smooth="false" url="/resources/icons/ExpandTreeView16.gif"/>
                                            </image>
                                        </ImageView>
                                    </graphic>
                                </Button>
                                <Button fx:id="collapseAllButton" maxHeight="18.0" maxWidth="18.0" minHeight="18.0"
                                        minWidth="18.0" mnemonicParsing="false" prefHeight="18.0" prefWidth="18.0">
                                    <graphic>
                                        <ImageView fitHeight="16.0" fitWidth="16.0" pickOnBounds="true"
                                                   preserveRatio="true">
                                            <image>
                                                <Image smooth="false" url="/resources/icons/CollapseTreeView16.gif"/>
                                            </image>
                                        </ImageView>
                                    </graphic>
                                </Button>
                                <Separator/>
                                <Button fx:id="showButton" maxHeight="18.0" maxWidth="18.0" minHeight="18.0"
                                        minWidth="18.0" mnemonicParsing="false" prefHeight="18.0" prefWidth="18.0">
                                    <graphic>
                                        <ImageView fitHeight="16.0" fitWidth="16.0" pickOnBounds="true"
                                                   preserveRatio="true">
                                            <image>
                                                <Image smooth="false"
                                                       url="/resources/icons/sun/toolbarButtonGraphics/general/Preferences16.gif"/>
                                            </image>
                                        </ImageView>
                                    </graphic>
                                </Button>

                            </items>
                        </ToolBar>
                        <TreeView fx:id="treeView" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                                  minHeight="0.0" minWidth="0.0" VBox.vgrow="ALWAYS"/>
                    </children>
                </VBox>
                <TabPane fx:id="tabPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                         tabClosingPolicy="UNAVAILABLE">
                    <opaqueInsets>
                        <Insets/>
                    </opaqueInsets>
                    <padding>
                        <Insets bottom="2.0" left="2.0" right="2.0" top="2.0"/>
                    </padding>
                </TabPane>
            </items>
        </SplitPane>
    </center>
    <bottom>
        <ToolBar fx:id="bottomToolBar" prefHeight="40.0" prefWidth="200.0" style="-fx-border-color: lightgray;"
                 BorderPane.alignment="CENTER"/>
    </bottom>
</BorderPane>
