#nexus
begin taxa;
dimensions ntax=5;
taxlabels
a
b
c
d
e;
end;

begin st_networks;
format type=SplitsNetwork partial=yes;
translate
1 a,
2 b,
3 c,
4 d,
5 e;
network first=
{GRAPH
{NODES {1 t=1 {label=AAA}}  {2 t=2 {label=BBB}}  {3 t=3 {label=CCC}}  {4 t=4 {label=DDD}}  {5 t=5 {label=EEEE}} {6} {7} {8}}
{EDGES {1 6 s=1} {2 6 s=2} {6 7 s=3} {3 7 s=4} {7 8 s=5} {4 8 s=6} {5 8 s=7}}
};
network second=
{GRAPH
{NODES {1 t=1 {label=AAA}}  {2 t=2 {label=BBB}}}
{EDGES {1 2 s=1 {label=Single}}}
};
end;

